{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "defaultPalette": true,
      "customDuotone": true,
      "customGradient": true,
      "palette": [
        {
          "name": "Grey",
          "slug": "grey",
          "color": "#555555"
        },
        {
          "name": "Light Grey",
          "slug": "light-grey",
          "color": "#f3f3f3"
        },
        {
          "name": "Medium Grey",
          "slug": "medium-grey",
          "color": "#d8d8d8"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        }
      ]
    },
    "layout": {
      "contentSize": "100rem",
      "wideSize": "120rem"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontWeight": true,
      "lineHeight": true,
      "customFontSize": true,
      "fontSizes": [
        {
          "slug": "tiny",
          "size": "0.675rem",
          "name": "Tiny"
        },
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "1rem",
          "name": "Normal"
        },
        {
          "slug": "large",
          "size": "1.125rem",
          "name": "Large"
        },
        {
          "slug": "extra-large",
          "size": "1.5rem",
          "name": "Extra Large"
        },
        {
          "slug": "gigantic",
          "size": "3rem",
          "name": "Gigantic"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "\"Helvetica Neue\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", sans-serif",
          "slug": "body-font",
          "name": "Body"
        },
        {
          "fontFamily": "\"Helvetica Neue\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", sans-serif",
          "slug": "heading-font",
          "name": "Heading"
        }
      ]
    },
    "blocks": {
      "core/group": {
        "spacing": {
          "padding": true,
          "margin": true
        }
      }
    }
  },
  "styles": {
    "typography": {
      "lineHeight": ""
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}
